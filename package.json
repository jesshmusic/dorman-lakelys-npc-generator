{
  "name": "dorman-lakelys-npc-generator",
  "version": "1.2.1",
  "description": "A FoundryVTT module for generating NPCs.",
  "main": "scripts/main.js",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "clean": "rimraf scripts/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci --maxWorkers=2",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"**/*.json\" \"**/*.md\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"**/*.json\" \"**/*.md\"",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@league-of-foundry-developers/foundry-vtt-types": "^12.331.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.0.0",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4"
  },
  "keywords": [
    "foundry-vtt",
    "module",
    "npc",
    "generator",
    "typescript"
  ],
  "author": "jesshmusic",
  "license": "MIT"
}
